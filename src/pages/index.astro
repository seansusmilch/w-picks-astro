---
import Layout from '../layouts/Layout.astro';
import Matchup from '@/components/Matchup.astro';
import { PickForm } from '@/components/Picks/PickForm';
const pb = Astro.locals.pb;

const matchups = await pb.collection('matchups').getFullList();
console.log(matchups);
---

<Layout title='W Picks | Home'>
  <Matchup matchup={matchups[0]} />
  <PickForm matchup={matchups[0]} client:load />
</Layout>
